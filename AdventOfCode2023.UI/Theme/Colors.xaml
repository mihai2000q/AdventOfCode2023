<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background Colors-->
    <SolidColorBrush x:Key="BackgroundColor">#1B2129</SolidColorBrush>
    <SolidColorBrush x:Key="OnBackgroundColor">#E0E1F1</SolidColorBrush>
    
    <!--Window Border Colors-->
    <Color x:Key="WinBorderColor1">#6D2FFF</Color>
    
    <!--Color Palette-->
    <SolidColorBrush x:Key="PrimaryColor" Color="#16417C"/>
    <SolidColorBrush x:Key="OnPrimaryColor" Color="#E0E1F1"/>
    
    <SolidColorBrush x:Key="UnfocusedColor" Color="#4E627C"/>
</ResourceDictionary>